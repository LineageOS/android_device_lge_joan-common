allow hal_power_default {
    sysfs_devfreq
    sysfs_devices_system_cpu
    sysfs_input
    sysfs_kgsl
    sysfs_msm_subsys
}:dir r_dir_perms;
allow hal_power_default {
    sysfs_devfreq
